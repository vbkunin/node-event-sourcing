{
  "name": "buns-with-raisins-command-api",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf ./build ./dist && npx tsc --build && npx ts-project-bundle --out=build",
